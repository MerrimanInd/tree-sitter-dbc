{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "dbc",
      "camelcase": "Dbc",
      "title": "CAN DBC",
      "scope": "source.dbc",
      "file-types": [
        "dbc"
      ],
      "injection-regex": "^dbc$",
      "class-name": "TreeSitterDbc"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "CANbus DBC bus descriptor file format",
    "authors": [
      {
        "name": "Xander Cesari",
        "email": "xander@merriman.industries",
        "url": "https://merriman.industries/"
      }
    ],
    "links": {
      "repository": "https://github.com/MerrimanInd/tree-sitter-dbc",
      "funding": "https://github.com/MerrimanInd/tree-sitter-dbc"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}